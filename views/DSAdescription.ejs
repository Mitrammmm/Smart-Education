        <!-- Description Section -->
        <section class="topics-detail-section section-padding" id="topics-detail">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-12 m-auto">
                        <h3 class="mb-4">Introduction to Data Structures and Algorithms</h3>
                        <p>Data structures and algorithms are fundamental concepts in computer science that involve organizing
                            and processing data efficiently.</p>
                        <!-- Additional content -->
                    </div>
                </div>
            </div>
        </section>

        <!-- Interview Question Section -->
        <div class="container">
            <h2 class="text-center mb-5">Interview Questions</h2>
            <div class="row bg-light">
                <div class="col-lg-6 col-md-6 col-12">
                    <section class="interview-section section-padding">
                        <h3>Data Structures Interview Questions</h3>
                        <div class="accordion" id="accordionDataStructures">
                            <!-- Data Structures Interview Questions -->
                            <div class="accordion-item">
                                <h2 class="accordion-header" id="dsQuestion1">
                                    <button class="accordion-button" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#dsAnswer1" aria-expanded="true" aria-controls="dsAnswer1">
                                        Question 1: What is a linked list?
                                    </button>
                                </h2>
                                <div id="dsAnswer1" class="accordion-collapse collapse show"
                                    aria-labelledby="dsQuestion1" data-bs-parent="#accordionDataStructures">
                                    <div class="accordion-body">
                                        Answer 1: A linked list is a linear data structure where elements are
                                        stored in a sequence, and each element points to the next one using
                                        pointers.
                                    </div>
                                </div>
                            </div>
                            <!-- Add more questions here -->
                        </div>
                    </section>
                </div>
                <div class="col-lg-6 col-md-6 col-12">
                    <section class="interview-section section-padding">
                        <h3>Algorithms Interview Questions</h3>
                        <div class="accordion" id="accordionAlgorithms">
                            <!-- Algorithms Interview Questions -->
                            <div class="accordion-item">
                                <h2 class="accordion-header" id="algQuestion1">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#algAnswer1" aria-expanded="false" aria-controls="algAnswer1">
                                        Question 1: What is a sorting algorithm?
                                    </button>
                                </h2>
                                <div id="algAnswer1" class="accordion-collapse collapse"
                                    aria-labelledby="algQuestion1" data-bs-parent="#accordionAlgorithms">
                                    <div class="accordion-body">
                                        Answer 1: A sorting algorithm is an algorithm that arranges elements in a
                                        specific order, such as ascending or descending order.
                                    </div>
                                </div>
                            </div>
                            <!-- Add more questions here -->
                        </div>
                    </section>
                </div>
            </div>
        </div>

        <center>
            <button type="button" class="btn btn-primary text-light" data-bs-toggle="modal"
                data-bs-target="#resource">Add a Resource</button>
        </center>

        <section class="downloads-section section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-12">
                        <h4 class="mb-4">Notes Download</h4>
                        <!-- notes -->
                        <% r.forEach(resource=> { %>
                            <% if (resource.category=='notes' && resource.topic=='dsa' ) {%>
                                <div class="card">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <img src="images/pdf.jpg" style="height: 120px; width: 120px;"
                                                    class="card-img-right" alt="Logo">
                                            </div>
                                            <div class="col-md-8">
                                                <h5 class="card-title">
                                                    <%= resource.title %>
                                                </h5>
                                                <p class="card-text">
                                                    <%= resource.description %>
                                                </p>
                                                <a href="<%= resource.link %>" class="btn btn-primary"
                                                    target="_blank">Download</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% }%>
                        <% }); %>
                    </div>
                    <div class="col-lg-6 col-md-6 col-12">
                        <h4 class="mb-4">View Projects</h4>
                        <!-- Projects -->
                        <% r.forEach(resource=> { %>
                            <% if(resource.category=='project' && resource.topic=='dsa' ) { %>
                                <div class="card">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <img src="images/github.jpg" style="height: 120px; width: 120px;"
                                                    class="card-img-right" alt="Logo">
                                            </div>
                                            <div class="col-md-8">
                                                <h5 class="card-title">
                                                    <%= resource.title %>
                                                </h5>
                                                <p class="card-text">
                                                    <%= resource.description %>
                                                </p>
                                                <a href="<%= resource.link %>" class="btn btn-primary"
                                                    target="_blank">GITHUB</a>
                                                <!-- <% if (resource.liveLink) { %>
                                    <a href="<%= resource.liveLink %>" class="btn btn-success" target="_blank">Live</a>
                                    <% } %> -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                        <% }); %>
                    </div>
                </div>
            </div>
        </section>

        <!-- Last Description Section -->
        <section class="description-section section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-12 m-auto text-center">
                        <h2 class="mb-4">Why Data Structures and Algorithms Matter</h2>
                        <blockquote class="mx-auto" style="max-width: 80%;">
                            <p>Data structures and algorithms form the backbone of computer science, enabling efficient
                                problem-solving and software development.</p>
                            <p>Understanding these concepts is essential for building robust and scalable software
                                applications that meet real-world needs.</p>
                            <div class="text-center mt-5">
                                <a target="_blank" href="https://www.geeksforgeeks.org/data-structures/" class="btn custom-btn custom-border-btn smoothscroll me-4">Read
                                    More</a>
                            </div>
                        </blockquote>
                    </div>
                </div>
            </div>
        </section>
    </main>

    <!--Resource Modal -->
    <div class="modal fade" id="resource" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h1 class="modal-title fs-5" id="exampleModalLabel">
                        Resource Upload
                    </h1>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form action="insertResource" method="post">
                        <div class="mb-3">
                            <label for="">Title</label>
                            <input type="text" name="title" class="form-control" />
                        </div>
                        <div class="mb-3">
                            <label for="">Description</label>
                            <input type="text" name="description" class="form-control" />
                        </div>
                        <div class="mb-3">
                            <label for="">Link</label>
                            <input type="text" name="link" class="form-control" />
                        </div>
                        <div class="mb-3">
                            <label for="">Category :</label>
                            <input type="radio" name="category" value="notes" />Notes
                            <input type="radio" name="category" value="project" />Project
                        </div>
                        <div class="mb-3">
                            <label for="">Topic :</label>
                            <input type="radio" name="topic" value="webdesign" />Web Design
                            <input type="radio" name="topic" value="dsa" />DSA
                            <input type="radio" name="topic" value="machinelearning" />Machine Learning

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-success">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!--Resource Modal End-->

