    <style>
        /* Interview Section */
        .interview-section {
            background-color: #f8f9fa;
            padding-top: 50px;
            padding-bottom: 50px;
        }

        .interview-section h3 {
            color: #333;
        }

        /* Accordion */
        .accordion-item {
            margin-bottom: 20px;
        }

        .accordion-button {
            background-color: #fff;
            color: #333;
            border: none;
            text-align: left;
            width: 100%;
            padding: 15px;
            cursor: pointer;
            transition: background-color 0.3s ease;
        }

        .accordion-button:hover {
            background-color: #f0f0f0;
        }

        .accordion-body {
            background-color: #fff;
            color: #555;
            padding: 15px;
            border: 1px solid #ddd;
            border-top: none;
        }

        /* Downloads Section */
        .downloads-section {
            background-color: #f8f9fa;
            padding-top: 50px;
            padding-bottom: 50px;
        }

        .downloads-section h4 {
            color: #333;
        }
    </style>
</head>

