    <style>
        /* Interview Section */
        .interview-section {
            background-color: #f8f9fa;
            /* Set background color */
            padding-top: 50px;
            padding-bottom: 50px;
        }

        .interview-section h3 {
            color: #333;
            /* Set heading color */
        }

        /* Accordion */
        .accordion-item {
            margin-bottom: 20px;
        }

        .accordion-button {
            background-color: #fff;
            /* Set accordion button background color */
            color: #333;
            /* Set accordion button text color */
            border: none;
            text-align: left;
            width: 100%;
            padding: 15px;
            cursor: pointer;
            transition: background-color 0.3s ease;
        }

        .accordion-button:hover {
            background-color: #f0f0f0;
            /* Set hover background color */
        }

        .accordion-body {
            background-color: #fff;
            /* Set accordion body background color */
            color: #555;
            /* Set accordion body text color */
            padding: 15px;
            border: 1px solid #ddd;
            /* Set border color */
            border-top: none;
        }

        /* Downloads Section */
        .downloads-section {
            background-color: #f8f9fa;
            /* Set background color */
            padding-top: 50px;
            padding-bottom: 50px;
        }

        .downloads-section h4 {
            color: #333;
            /* Set heading color */
        }
    </style>

